# Neon PostgreSQL connection string
DATABASE_URL=postgresql://user:password@host/dbname?sslmode=require

# Google OAuth — from https://console.cloud.google.com
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# BetterAuth secret — any long random string
BETTER_AUTH_SECRET=change-me-to-a-long-random-secret

# URL of the Vite dev server (used by the Hono server for CORS + redirects)
CLIENT_ORIGIN=http://localhost:5173
